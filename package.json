{
  "name": "koupon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "cd server && npm install && cd ../client && npm install && npm run build && cd ..",
    "start": "cd server && npm start",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run start",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm run start\""
  },
  "engines": {
    "node": "13.6.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.0.2",
    "query-string": "^6.10.1"
  },
